<h1>People to Invite</h1>

<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Alias</th>
    </tr>
@foreach (var attendee in ViewBag.attendees)
{
    <tr>
        <td>@attendee.FirstName</td>
        <td>@attendee.LastName</td>
        <td>@attendee.Alias</td>
    </tr>
}
</table>